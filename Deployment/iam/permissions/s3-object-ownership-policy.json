{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowFullControlOfProcessedObjects",
            "Effect": "Allow",
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::manara-my-processed-bucket/*",
            "Condition": {
                "StringEquals": {
                    "s3:x-amz-acl": "bucket-owner-full-control"
                }
            }
        }
    ]
} 